{
  "slos": [
    {
      "id": "slo-1",
      "name": "Model Response Time SLO",
      "description": "95% of FIBO outfit generation requests should complete within 3 seconds",
      "type": "metric",
      "target_threshold": 95.0,
      "warning_threshold": 97.0,
      "timeframe": "30d",
      "type_id": 0,
      "query": {
        "numerator": "sum:fibo.generation.duration{application:aurelians_closet,success:true}.as_count() - sum:fibo.generation.duration{application:aurelians_closet,success:true,duration:>3000}.as_count()",
        "denominator": "sum:fibo.generation.duration{application:aurelians_closet,success:true}.as_count()"
      },
      "tags": [
        "service:aurelians-closet",
        "team:ai-engineering",
        "category:performance"
      ],
      "monitor_ids": [],
      "thresholds": [
        {
          "target": 95.0,
          "warning": 97.0,
          "timeframe": "30d"
        },
        {
          "target": 95.0,
          "warning": 97.0,
          "timeframe": "7d"
        }
      ],
