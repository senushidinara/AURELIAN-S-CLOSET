{
  "title": "Aurelian's Closet - AI Fashion Platform Observability",
  "description": "Comprehensive monitoring dashboard for AI-powered fashion generation with Vertex AI/Gemini integration",
  "layout_type": "ordered",
  "is_read_only": false,
  "notify_list": [
    "ai-team@company.com"
  ],
  "template_variables": [
    {
      "name": "environment",
      "prefix": "env",
      "available_values": ["production", "staging", "development"],
      "default": "production"
    },
    {
      "name": "model",
      "prefix": "model",
      "available_values": ["gemini-2.5-flash", "gemini-pro"],
      "default": "*"
